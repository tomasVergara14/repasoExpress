<%- include('./partials/head') %>
<%- include('./partials/header') %>  

<body>
    <div class="latest">
        <div class="latestTitle">
            <h1>Detalle del Producto <%=productId.name%> </h1>
        </div>     
            <section class="latestCards">
                <figure>
                <a href="/products/detail/<%=productId.id%>"> <img class="latestCardsImage" src="/images/img/products/<%=productId.image%>" alt="<%=productId.name%>"></a>
                </figure>
                <article class="latestInfo">
                <article class="headDescription">
                    <span class="off">Discount %15 OFF </span>
                    <h3 class="price"><%=productId.price%></h3>   
                </article>
                <p class="description"> <%=productId.name%> </p>
                </article>
            </section>
        
    </div>    
</body>

<%- include('./partials/footer') %> -->